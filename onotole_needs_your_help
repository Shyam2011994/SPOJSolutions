#include <set>
#include <map>
#include <list>
#include <cmath>
#include <ctime>
#include <cstdlib>
#include <algorithm>
using namespace std;
typedef long long LL;
int main() {
	LL N, num = 0, res, inp;
	scanf("%lld",&N);
	for(int i = 0 ; i < N ; i++)
	{
		scanf("%lld",&inp);
		num ^= inp;
	}
	printf("%lld\n",num);
	return 0;
}
